server {
  listen 80;

  location ~ /api/(.*) {
    include proxy_params;
    proxy_pass http://0.0.0.0:5002/api/$1;
  }
  location ~ /(.*) {
    proxy_pass http://0.0.0.0:5003/2-hbnb/$1;
  }
  location ^~ /static/ {
    include  /etc/nginx/mime.types;
    root /home/ubuntu/AirBnB_clone_v4/web_dynamic/;
  }
}

